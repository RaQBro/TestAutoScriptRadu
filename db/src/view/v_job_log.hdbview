VIEW "sap.plc.extensibility::template_application.v_job_log"
AS
SELECT 
	JOB_ID,
	JOB_NAME,
	USER_ID,
	IS_ONLINE_MODE,
	REQUEST_BODY,
	RESPONSE_BODY,
	SAP_JOB_ID,
	SAP_JOB_SCHEDULE_ID,
	SAP_JOB_RUN_ID,
	START_TIMESTAMP,
	END_TIMESTAMP,
	JOB_STATUS
FROM "sap.plc.extensibility::template_application.t_job_log"