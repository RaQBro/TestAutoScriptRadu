{
	"welcomeFile": "webapp/index.html",
	"authenticationMethod": "route",
	"logout": {
		"logoutEndpoint": "/logout",
		"logoutPage": "/webapp/auth/logoff.html"
	},
	"routes": [{
		"source": "^/service/(.*).xsodata(.*)$",
		"destination": "tapp_srv_api",
		"authenticationType": "xsuaa"
	}, {
		"source": "^/service/(.*).xsodata/\\$metadata(.*)$",
		"destination": "tapp_srv_api",
		"authenticationType": "xsuaa"
	},{
		"source": "^/standard/plc/(.*)$",
		"destination": "tapp_srv_api",
		"authenticationType": "xsuaa"
	}, {
		"source": "^/extensibility/plc/(.*)$",
		"destination": "tapp_srv_api",
		"authenticationType": "xsuaa"
	}, {
		"source": "^/scheduler/job/(.*)$",
		"destination": "tapp_srv_api",
		"authenticationType": "xsuaa"
	}, {
		"source": "^/secure/store/(.*)$",
		"destination": "tapp_srv_api",
		"authenticationType": "xsuaa"
	}, {
		"source": "^/webapp/auth/logoff.html$",
		"localDir": "resources",
		"authenticationType": "none",
		"csrfProtection": false
	}, {
		"source": "^/webapp/img/(.*).png",
		"localDir": "resources",
		"authenticationType": "none",
		"csrfProtection": false
	}, {
		"source": "^/webapp/index.html",
		"localDir": "resources",
		"authenticationType": "xsuaa",
		"csrfProtection": true,
		"cacheControl": "no-cache, no-store, must-revalidate"
	}, {
		"source": "^/webapp/(.+)$",
		"localDir": "resources",
		"authenticationType": "xsuaa",
		"csrfProtection": true
	}, {
		"source": "^/webapp/",
		"localDir": "resources",
		"authenticationType": "xsuaa"
	}]
}