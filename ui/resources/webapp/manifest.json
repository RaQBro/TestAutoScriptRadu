{
	"_version": "1.5.0",
	"sap.app": {
		"id": "webapp.ui",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{shellBarTitle}}",
		"description": "{{shellBarSecondTitle}}",
		"sourceTemplate": {
			"id": "hanatemplates.basicSAPUI5ApplicationProject",
			"version": "0.0.0"
		},
		"dataSources": {
			"toolBarMessages": {
				"uri": "toolBarMessages/ToolbarMessagesModel.json",
				"type": "JSON"
			},
			"serviceDS": {
				"uri": "/service/odataService.xsodata",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0"
				}
			}
		}
	},

	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "img/favicon.ico",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_fiori_3",
			"sap_horizon"
		]
	},

	"sap.ui5": {
		"rootView": {
			"viewName": "webapp.ui.view.Main",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"dependencies": {
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.f": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": false
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "webapp.ui.i18n.i18n"
				}
			},
			"serviceModel": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "TwoWay",
					"defaultCountMode": "Inline",
					"useBatch": false,
					"defaultUpdateMethod": "PUT"
				},
				"dataSource": "serviceDS",
				"preload": false
			},
			"toolBarMessagesModel": {
				"type": "sap.ui.model.json.JSONModel",
				"dataSource": "toolBarMessages"
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "webapp.ui.view",
				"controlAggregation": "pages",
				"controlId": "app"
			},
			"routes": [{
				"name": "view",
				"pattern": "",
				"target": ["view"]
			}, {
				"name": "defaultValues",
				"pattern": "defaultValues",
				"target": ["defaultValues"]
			}, {
				"name": "applicationSettings",
				"pattern": "applicationSettings",
				"target": ["applicationSettings"]
			}, {
				"name": "jobs",
				"pattern": "jobs",
				"target": ["jobs"]
			}, {
				"name": "messages",
				"pattern": "messages/:jobID:",
				"target": ["messages"]
			}, {
				"name": "error",
				"pattern": "error",
				"target": ["error"]
			}],
			"targets": {
				"view": {
					"viewId": "view",
					"viewName": "View",
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true
				},
				"defaultValues": {
					"viewId": "defaultValuesView",
					"viewName": "DefaultValues",
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true
				},
				"applicationSettings": {
					"viewId": "applicationSettingsView",
					"viewName": "ApplicationSettings",
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true
				},
				"jobs": {
					"viewName": "Jobs",
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true
				},
				"messages": {
					"viewName": "Messages",
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true
				},
				"error": {
					"viewId": "errorView",
					"viewName": "Error",
					"viewType": "XML",
					"transition": "show",
					"clearAggregation": true
				}
			}
		}
	}
}