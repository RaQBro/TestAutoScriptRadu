<mvc:View id="view" controllerName="webapp.ui.controller.View" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:core="sap.ui.core"
	displayBlock="true">
	<Page showHeader="false" showFooter="true" showNavButton="false" floatingFooter="false" showSubHeader="false">
		<content>
			<Button id="jobStartOnlineBtn" text="{i18n>jobOnlineBtn}" visible="true" press="onOnlinePress" type="Emphasized" enabled="true"/>
			<Button id="jobStartOfflineBtn" text="{i18n>jobOfflineBtn}" visible="true" press="onOfflinePress" type="Emphasized" enabled="true"/>
		</content>
		<footer>
			<core:Fragment fragmentName="webapp.ui.toolBarMessages.ToolbarMessages" type="XML"/>
		</footer>
	</Page>
</mvc:View>